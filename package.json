{
  "name": "nw-websocket-project",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev:web": "npm --prefix ./apps/web run dev",
    "dev:api": "npm --prefix ./apps/api run dev",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "start:web": "npm --prefix ./apps/web start",
    "start:api": "npm --prefix ./apps/api start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "tailwindcss": "^4.1.16"
  }
}
